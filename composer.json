{
    "name":         "tadcka/mapper-bundle",
    "homepage":     "https://github.com/tadcka/MapperBundle",
    "description":  "Mapper bundle on Symfony2",
    "keywords":     ["mapper", "mapper manager"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors": [
        {
            "name":   "Tadas Gliaubicas",
            "email":  "tadcka89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",

        "symfony/framework-bundle" : ">=2.3",

        "jms/serializer-bundle": "~0.13",
        "friendsofsymfony/jsrouting-bundle": "~1.5",

        "tadcka/mapper": "0.2.*@dev",
        "tadcka/jstree-bundle": "~0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "doctrine/doctrine-bundle": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Tadcka\\Bundle\\MapperBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
